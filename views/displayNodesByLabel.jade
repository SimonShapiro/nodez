html
	h1 Nodes with label='#{data.label}': #{data.json.length} found
	table
		each n, index in data.json
			tr
				td
					b= index+1   
				//td= n.self
				//each d in n.data
				//td= d
				//td=	(typeof n)
				if (data.label=="Meta")
					td
						a(href=n.data.name)  #{n.data.name}
				else
					td= n.data.name
				td= n.id
				td
					a(href=n.verbose) Verbose
				td
					a(href=n.navigate) Navigate
	hr
